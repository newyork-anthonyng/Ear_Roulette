{
  "name": "ear_roulette",
  "version": "1.0.0",
  "engines": {
    "node": "=4.2.2",
    "npm": "=2.14.7"
  },
  "description": "### Description Ear Roulette is a jukebox for people with short attention spans. All of the tracks in the jukebox last for 30 seconds before the next one is played. You will never be bored of music again.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "mocha --compilers js:babel-core/register --require ./test/frontend/test_helper.js \"test/frontend/**/*.@(jsx)\" --watch --watch-extensions jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newyork-anthonyng/Ear_Roulette.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/newyork-anthonyng/Ear_Roulette/issues"
  },
  "homepage": "https://github.com/newyork-anthonyng/Ear_Roulette#readme",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "jsonwebtoken": "^5.4.1",
    "mongoose": "^4.2.9",
    "morgan": "^1.6.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.4.1",
    "chai-http": "^1.0.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
